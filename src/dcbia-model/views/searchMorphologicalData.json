{
    "_id": "_design/searchMorphologicalData",
    "language": "javascript",
    "views": {
        "patientId": {
            "map": "function(doc) {\n\tif(doc.type === \"morphologicalData\"){\n\t\temit(doc.patientId, doc._id);\t\n\t}\n}\n"
        },
        "collectionName": {
            "map": "function(doc) {\n\tif(doc.type === \"morphologicalDataCollection\"){\n\t\temit(doc.name, doc.items);\t\n\t}\n}\n"
        },
        "patientIdDate": {
            "map": "function(doc) {\n\tif(doc.type === \"morphologicalData\" && doc.date){\n\t\temit([doc.patientId, doc.date], doc._id);\t\n\t}\n}"
        },
        "date": {
            "map": "function(doc) {\n\tif(doc.type === \"morphologicalData\" && doc.date){\n\t\temit(doc.date, doc._id);\t\n\t}\n}"
        }
    }
}